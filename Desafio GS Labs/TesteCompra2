    driver.FindElement(By.CssSelector(".ajax_cart_product_txt:nth-child(3)")).Click();
    // 11 | click | css=.standard-checkout > span | 
    driver.FindElement(By.CssSelector(".standard-checkout > span")).Click();
    // 12 | click | id=login_form | 
    driver.FindElement(By.Id("login_form")).Click();
    // 13 | click | id=email | 
    driver.FindElement(By.Id("email")).Click();
    // 14 | type | id=email | sreginavieira@gmail.com
    driver.FindElement(By.Id("email")).SendKeys("sreginavieira@gmail.com");
    // 15 | click | id=passwd | 
    driver.FindElement(By.Id("passwd")).Click();
