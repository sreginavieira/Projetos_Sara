  [Fact]
  public void Compra2() {
    // Test name: Compra2
    // Step # | name | target | value
    // 1 | open | http://automationpractice.com/index.php | 
    driver.Navigate().GoToUrl("http://automationpractice.com/index.php");
    // 2 | setWindowSize | 1382x744 | 
    driver.Manage().Window.Size = new System.Drawing.Size(1382, 744);
    // 3 | click | css=.hovered .product-name | 
    driver.FindElement(By.CssSelector(".hovered .product-name")).Click();
    // 4 | click | id=group_1 | 
    driver.FindElement(By.Id("group_1")).Click();
    // 5 | select | id=group_1 | label=L
    {
      var dropdown = driver.FindElement(By.Id("group_1"));
      dropdown.FindElement(By.XPath("//option[. = 'L']")).Click();
    }
    // 6 | click | id=group_1 | 
    driver.FindElement(By.Id("group_1")).Click();
    // 7 | click | css=.exclusive > span | 
    driver.FindElement(By.CssSelector(".exclusive > span")).Click();
    // 8 | click | css=.button-medium > span | 
    driver.FindElement(By.CssSelector(".button-medium > span")).Click();
    // 9 | click | css=.standard-checkout > span | 
    driver.FindElement(By.CssSelector(".standard-checkout > span")).Click();
